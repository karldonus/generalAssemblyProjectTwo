<div class="inquiry-form">
<%= form_for @rental, html: { multipart: true} do |f| %>
  <div><%= f.label :image, "Image File" %></div>
  <div><%= f.file_field :image %></div>
  <div><%= image_tag @rental.image.url(:thumb)%></div>
  <div><%= f.label :delete_image, "Delete Image" %>
    <%= f.check_box :delete_image %>
    <%= f.submit "Confirm", class: 'btn btn-info btn-md'%></div>
  <div><%= f.label :property_name, "Property Name" %></div>
  <div><%= f.text_field :property_name %></div>
  <div><%= f.label :agent, "Listing Agent" %></div>
  <div><%= f.text_field :agent %></div>
  <div><%= f.label :address, "Street Address" %></div>
  <div><%= f.text_field :address %></div>
  <div><%= f.label :city, "City" %></div>
  <div><%= f.text_field :city %></div>
  <div><%= f.label :state, "State" %></div>
  <div><%= f.text_field :state %></div>
  <div><%= f.label :zipcode, "Zipcode" %></div>
  <div><%= f.text_field :zipcode %></div>
  <div><%= f.label :monthly_rent, "Monthly Rent" %></div>
  <div><%= f.text_field :monthly_rent %></div>
  <div><%= f.label :bed_num, "Number of Beds" %></div>
  <div><%= f.text_field :bed_num %></div>
  <div><%= f.label :bath_num, "Number of Baths" %></div>
  <div><%= f.text_field :bath_num %></div>
  <div><%= f.label :availability_date, "Date Available" %></div>
  <div><%= f.text_field :availability_date %></div>
  <div><%= f.label :client_name, "Client Name" %></div>
  <div><%= f.text_field :client_phone %></div>
  <div><%= f.label :client_email, "Client Email" %></div>
  <div><%= f.text_field :client_email %></div>
  <div><%= f.label :listing_notes, "Listing Notes (Public)" %></div>
  <div><%= f.text_area :listing_notes %></div>
  <div><%= f.label :admin_notes, "Agent Notes (Private)" %></div>
  <div><%= f.text_area :admin_notes %></div>

  <%= f.submit class: 'btn btn-info btn-md' %>

<% end  %>
</div>
